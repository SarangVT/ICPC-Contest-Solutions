  "prefix": "modexp",
  "body": [
    "ll mod_pow(ll exp, ll base, ll mod) {",
    "    base%=mod;",
    "    ll ans = 1;",
    "    while(exp > 0) {",
    "        if(exp & 1) ans = (ans*base)%mod;",
    "        base = (base*base)%mod;",
    "        exp >>= 1;",
    "    }",
    "    return ans;",
    "}",
    ""
